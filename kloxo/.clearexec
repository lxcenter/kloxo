
__my_pre_clearexec () {
	rm img; 
	mv lang ../
	if [ $1 = "grep" ] ; then
		/bin/mv tags ../__old.tag
	fi
	mv thirdparty ../my-thirdparty
}

__my_post_clearexec () {
	ln -sf ../../kloxo-img/ img; 
	mv ../lang .
	if [ $1 = "grep" ] ; then
		/bin/mv ../__old.tag tags 2>/dev/null
	fi
	mv ../my-thirdparty ./thirdparty
}

